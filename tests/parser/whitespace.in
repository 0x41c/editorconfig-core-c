; test whitespace usage

; no whitespace
[test1.c]
key=value

; spaces around equals
[test2.c]
key = value

; lots of space after equals
[test3.c]
key  =   value

; spaces before property name
[test4.c]
  key=value

; spaces after property value
[test5.c]
key=value  

; blank lines between properties
[test6.c]

key1=value1

key2=value2

; spaces before section names
[  test7.c]
key7 = value7

; spaces after section names
[test8.c        ]
key8 = value8
